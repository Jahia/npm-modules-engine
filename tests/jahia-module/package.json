{
  "name": "@jahia/npm-module-example",
  "description": "Jahia NPM Test module",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Jahia Solutions Group SA",
  "scripts": {
    "postinstall": "yarn dlx @yarnpkg/sdks vscode",
    "build": "webpack --mode=development && webpack --config webpack.client.js --mode=development",
    "deploy": "jahia-deploy pack",
    "watch": "yarn build --env deploy=true --watch",
    "lint": "eslint .",
    "test": "yarn lint"
  },
  "jahia": {
    "maven": {
      "groupId": "org.jahia.modules.npm",
      "distributionManagement": {
        "repository": {
          "id": "jahia-releases",
          "url": "https://devtools.jahia.com/nexus/content/repositories/jahia-releases"
        },
        "snapshotRepository": {
          "id": "jahia-snapshots",
          "url": "https://devtools.jahia.com/nexus/content/repositories/jahia-snapshots"
        }
      }
    },
    "module-dependencies": "default",
    "module-type": "templatesSet",
    "required-version": "8.2.0.0-SNAPSHOT",
    "server": "dist/main.js",
    "static-resources": "/css,/images,/javascript"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.21.5",
    "@jahia/eslint-config": "^2.1.0",
    "@jahia/scripts": "^1.3.3",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.43.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "extra-watch-webpack-plugin": "^1.0.3",
    "handlebars-loader": "^1.7.3",
    "styled-jsx": "^5.1.2",
    "typescript": "^5.3.3",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin-next": "^2.3.1"
  },
  "resolutions": {
    "minimist": "^1.2.6"
  },
  "dependencies": {
    "@jahia/data-helper": "^1.0.3",
    "@jahia/js-server-engine": "^1.2.10",
    "handlebars": "^4.7.7",
    "react": "^18.2.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "yarn": ">=4.0.0"
  }
}
